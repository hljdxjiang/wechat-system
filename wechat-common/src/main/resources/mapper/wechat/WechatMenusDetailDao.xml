<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuoding.wechat.common.dao.wechat.WechatMenusDetailDao">

    <resultMap type="com.nuoding.wechat.common.entity.wechat.WechatMenusDetailEntity" id="WechatMenusDetailMap">
            <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="tenantId" column="tenant_id" jdbcType="VARCHAR"/>
        <result property="menuId" column="menu_id" jdbcType="VARCHAR"/>
        <result property="itemPid" column="item_pid" jdbcType="VARCHAR"/>
        <result property="itemId" column="item_id" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="lastModifiedTime" column="last_modified_time" jdbcType="TIMESTAMP"/>
        <result property="sort" column="sort" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Table_Name">
        wechat_menus_detail
    </sql>

    <sql id="Base_Column_List">
        id,tenant_id,menu_id,item_pid,item_id,create_time,last_modified_time,sort    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="WechatMenusDetailMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="WechatMenusDetailMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
                                                        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
                                                <if test="menuId != null and menuId !=''">and menu_id = #{menuId}</if>
                                                <if test="itemPid != null and itemPid !=''">and item_pid = #{itemPid}</if>
                                                <if test="itemId != null and itemId !=''">and item_id = #{itemId}</if>
                                                                                        <if test="sort != null and sort !=''">and sort = #{sort}</if>
                    </where>
    </select>

    <!--查询一条-->
    <select id="selectOne" resultMap="WechatMenusDetailMap">
    select
    <include refid="Base_Column_List"/>
    from
    <include refid="Table_Name"/>
    <where>
                                                        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
                                                <if test="menuId != null and menuId !=''">and menu_id = #{menuId}</if>
                                                <if test="itemPid != null and itemPid !=''">and item_pid = #{itemPid}</if>
                                                <if test="itemId != null and itemId !=''">and item_id = #{itemId}</if>
                                                                                        <if test="sort != null and sort !=''">and sort = #{sort}</if>
                        </where>
    </select>

    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="WechatMenusDetailMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
                                                                                                    <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
                                                                                                            <if test="menuId != null and menuId !=''">and menu_id like CONCAT(CONCAT('%',  #{menuId}), '%')</if>
                                                                                                            <if test="itemPid != null and itemPid !=''">and item_pid like CONCAT(CONCAT('%',  #{itemPid}), '%')</if>
                                                                                                            <if test="itemId != null and itemId !=''">and item_id like CONCAT(CONCAT('%',  #{itemId}), '%')</if>
                                                                                                                                                                    <if test="sort != null and sort !=''">and sort like CONCAT(CONCAT('%',  #{sort}), '%')</if>
                                                        </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from
        <include refid="Table_Name"/>
        <where>
                                                <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
                                        <if test="menuId != null and menuId !=''">and menu_id = #{menuId}</if>
                                        <if test="itemPid != null and itemPid !=''">and item_pid = #{itemPid}</if>
                                        <if test="itemId != null and itemId !=''">and item_id = #{itemId}</if>
                                                                <if test="sort != null and sort !=''">and sort = #{sort}</if>
            </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (tenant_id,menu_id,item_pid,item_id,sort)
        values
        (#{tenantId},#{menuId},#{itemPid},#{itemId},#{sort})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (        tenant_id,        menu_id,        item_pid,        item_id,        sort)
        values
        <foreach collection="entities" item="entity" separator=",">
            (            #{entity.tenantId},            #{entity.menuId},            #{entity.itemPid},            #{entity.itemId},            #{entity.sort})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (        tenant_id,        menu_id,        item_pid,        item_id,        sort)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.menuId},#{entity.itemPid},#{entity.itemId},#{entity.sort})
        </foreach>
        on duplicate key update
        tenant_id=values(tenant_id),
    menu_id=values(menu_id),
    item_pid=values(item_pid),
    item_id=values(item_id),
            sort=values(sort)
        </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update
        <include refid="Table_Name"/>
        <set>
    <if test="tenantId != null and tenantId != ''">tenant_id = #{tenantId},</if>
    <if test="menuId != null and menuId != ''">menu_id = #{menuId},</if>
    <if test="itemPid != null and itemPid != ''">item_pid = #{itemPid},</if>
    <if test="itemId != null and itemId != ''">item_id = #{itemId},</if>
    <if test="sort != null and sort != ''">sort = #{sort},</if>
        </set>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from
        <include refid="Table_Name"/>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </delete>

</mapper>

