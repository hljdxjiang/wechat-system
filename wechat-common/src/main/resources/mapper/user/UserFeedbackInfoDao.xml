    <!--查询单个-->
    <select id="queryById" resultMap="UserFeedbackInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="UserFeedbackInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="userId != null and userId !=''">and user_id = #{userId}</if>
            <if test="mobileNo != null and mobileNo !=''">and mobile_no = #{mobileNo}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="feedType != null and feedType !=''">and feed_type = #{feedType}</if>
            <if test="feedTitle != null and feedTitle !=''">and feed_title = #{feedTitle}</if>
            <if test="feedDetail != null and feedDetail !=''">and feed_detail = #{feedDetail}</if>
            <if test="feedImg != null and feedImg !=''">and feed_img = #{feedImg}</if>
        </where>
    </select>

    <!--查询一条-->
    <select id="selectOne" resultMap="UserFeedbackInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="userId != null and userId !=''">and user_id = #{userId}</if>
            <if test="mobileNo != null and mobileNo !=''">and mobile_no = #{mobileNo}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="feedType != null and feedType !=''">and feed_type = #{feedType}</if>
            <if test="feedTitle != null and feedTitle !=''">and feed_title = #{feedTitle}</if>
            <if test="feedDetail != null and feedDetail !=''">and feed_detail = #{feedDetail}</if>
            <if test="feedImg != null and feedImg !=''">and feed_img = #{feedImg}</if>
        </where>
    </select>

    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="UserFeedbackInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="userId != null and userId !=''">and user_id like CONCAT(CONCAT('%', #{userId}), '%')</if>
            <if test="mobileNo != null and mobileNo !=''">and mobile_no like CONCAT(CONCAT('%', #{mobileNo}), '%')</if>
            <if test="status != null and status !=''">and status like CONCAT(CONCAT('%', #{status}), '%')</if>
            <if test="feedType != null and feedType !=''">and feed_type like CONCAT(CONCAT('%', #{feedType}), '%')</if>
            <if test="feedTitle != null and feedTitle !=''">and feed_title like CONCAT(CONCAT('%', #{feedTitle}), '%')
            </if>
            <if test="feedDetail != null and feedDetail !=''">and feed_detail like CONCAT(CONCAT('%', #{feedDetail}),
                '%')
            </if>
            <if test="feedImg != null and feedImg !=''">and feed_img like CONCAT(CONCAT('%', #{feedImg}), '%')</if>
        </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
