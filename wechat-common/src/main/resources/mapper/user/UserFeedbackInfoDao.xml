            <if test="feedImg != null and feedImg !=''">and feed_img = #{feedImg}</if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (tenant_id,user_id,mobile_no,feed_type,feed_title,feed_detail,feed_img)
        values
        (#{tenantId},#{userId},#{mobileNo},#{feedType},#{feedTitle},#{feedDetail},#{feedImg})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( tenant_id, user_id, mobile_no, feed_type, feed_title, feed_detail, feed_img)
        values
        <foreach collection="entities" item="entity" separator=",">
            ( #{entity.tenantId}, #{entity.userId}, #{entity.mobileNo}, #{entity.feedType}, #{entity.feedTitle},
            #{entity.feedDetail}, #{entity.feedImg})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( tenant_id, user_id, mobile_no, status, feed_type, feed_title, feed_detail, feed_img)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.userId},#{entity.mobileNo},#{entity.status},#{entity.feedType},#{entity.feedTitle},#{entity.feedDetail},#{entity.feedImg})
        </foreach>
        on duplicate key update
        tenant_id=values(tenant_id),
        user_id=values(user_id),
        mobile_no=values(mobile_no),
        status=values(status),
        feed_type=values(feed_type),
        feed_title=values(feed_title),
        feed_detail=values(feed_detail),
        feed_img=values(feed_img)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
