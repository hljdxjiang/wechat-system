            <if test="userId != null and userId != ''">user_id = #{userId},</if>
            <if test="mobileNo != null and mobileNo != ''">mobile_no = #{mobileNo},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="feedType != null and feedType != ''">feed_type = #{feedType},</if>
            <if test="feedTitle != null and feedTitle != ''">feed_title = #{feedTitle},</if>
            <if test="feedDetail != null and feedDetail != ''">feed_detail = #{feedDetail},</if>
            <if test="feedImg != null and feedImg != ''">feed_img = #{feedImg},</if>
        </set>
        where id = #{id}
