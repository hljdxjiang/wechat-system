<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuoding.wechat.common.dao.user.UserPropertyDao">

    <resultMap type="com.nuoding.wechat.common.entity.user.UserPropertyEntity" id="UserPropertyMap">
            <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="tenantId" column="tenant_id" jdbcType="VARCHAR"/>
        <result property="userid" column="userid" jdbcType="VARCHAR"/>
        <result property="integral" column="integral" jdbcType="VARCHAR"/>
        <result property="species" column="species" jdbcType="VARCHAR"/>
        <result property="silverIcon" column="silver_icon" jdbcType="VARCHAR"/>
        <result property="balance" column="balance" jdbcType="DECIMAL"/>
        <result property="giftAmount" column="gift_amount" jdbcType="DECIMAL"/>
        <result property="createtime" column="createtime" jdbcType="TIMESTAMP"/>
        <result property="lastmodifiedtime" column="lastmodifiedtime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Table_Name">
        user_property
    </sql>

    <sql id="Base_Column_List">
        id,tenant_id,userid,integral,species,silver_icon,balance,gift_amount,createtime,lastmodifiedtime    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="UserPropertyMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="UserPropertyMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="id != null and id !=''">and id = #{id}</if>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="userid != null and userid !=''">and userid = #{userid}</if>
            <if test="integral != null and integral !=''">and integral = #{integral}</if>
            <if test="species != null and species !=''">and species = #{species}</if>
            <if test="silverIcon != null and silverIcon !=''">and silver_icon = #{silverIcon}</if>
            <if test="balance != null and balance !=''">and balance = #{balance}</if>
            <if test="giftAmount != null and giftAmount !=''">and gift_amount = #{giftAmount}</if>
            <if test="createtime != null and createtime !=''">and createtime = #{createtime}</if>
            <if test="lastmodifiedtime != null and lastmodifiedtime !=''">and lastmodifiedtime = #{lastmodifiedtime}</if>
        </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from
        <include refid="Table_Name"/>
        <where>
            <if test="id != null and id != ''">and id = #{id}</if>
            <if test="tenantId != null and tenantId != ''">and tenant_id = #{tenantId}</if>
            <if test="userid != null and userid != ''">and userid = #{userid}</if>
            <if test="integral != null and integral != ''">and integral = #{integral}</if>
            <if test="species != null and species != ''">and species = #{species}</if>
            <if test="silverIcon != null and silverIcon != ''">and silver_icon = #{silverIcon}</if>
            <if test="balance != null and balance != ''">and balance = #{balance}</if>
            <if test="giftAmount != null and giftAmount != ''">and gift_amount = #{giftAmount}</if>
            <if test="createtime != null and createtime != ''">and createtime = #{createtime}</if>
            <if test="lastmodifiedtime != null and lastmodifiedtime != ''">and lastmodifiedtime = #{lastmodifiedtime}</if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (tenant_id,userid,integral,species,silver_icon,balance,gift_amount,createtime,lastmodifiedtime)
        values
        (#{tenantId},#{userid},#{integral},#{species},#{silverIcon},#{balance},#{giftAmount},#{createtime},#{lastmodifiedtime})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (tenant_id,userid,integral,species,silver_icon,balance,gift_amount,createtime,lastmodifiedtime)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.userid},#{entity.integral},#{entity.species},#{entity.silverIcon},#{entity.balance},#{entity.giftAmount},#{entity.createtime},#{entity.lastmodifiedtime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (tenant_id,userid,integral,species,silver_icon,balance,gift_amount,createtime,lastmodifiedtime)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.userid},#{entity.integral},#{entity.species},#{entity.silverIcon},#{entity.balance},#{entity.giftAmount},#{entity.createtime},#{entity.lastmodifiedtime})
        </foreach>
        on duplicate key update
                tenant_id=values(tenant_id),
        userid=values(userid),
        integral=values(integral),
        species=values(species),
        silver_icon=values(silver_icon),
        balance=values(balance),
        gift_amount=values(gift_amount),
        createtime=values(createtime),
        lastmodifiedtime=values(lastmodifiedtime)
        </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update
        <include refid="Table_Name"/>
        <set>
    <if test="tenantId != null and tenantId != ''">tenant_id = #{tenantId},</if>
    <if test="userid != null and userid != ''">userid = #{userid},</if>
    <if test="integral != null and integral != ''">integral = #{integral},</if>
    <if test="species != null and species != ''">species = #{species},</if>
    <if test="silverIcon != null and silverIcon != ''">silver_icon = #{silverIcon},</if>
    <if test="balance != null and balance != ''">balance = #{balance},</if>
    <if test="giftAmount != null and giftAmount != ''">gift_amount = #{giftAmount},</if>
    <if test="createtime != null and createtime != ''">createtime = #{createtime},</if>
    <if test="lastmodifiedtime != null and lastmodifiedtime != ''">lastmodifiedtime = #{lastmodifiedtime},</if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from
        <include refid="Table_Name"/>
        where id = #{id}
    </delete>

</mapper>

