<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuoding.wechat.common.dao.sys.SysResourceInfoDao">

    <resultMap type="com.nuoding.wechat.common.entity.sys.SysResourceInfoEntity" id="SysResourceInfoMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="tenantId" column="tenant_id" jdbcType="VARCHAR"/>
        <result property="resourceId" column="resource_id" jdbcType="VARCHAR"/>
        <result property="resourceType" column="resource_type" jdbcType="VARCHAR"/>
        <result property="linkUrl" column="link_url" jdbcType="VARCHAR"/>
        <result property="status" column="status" jdbcType="INTEGER"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
        <result property="remark" column="remark" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="lastModifiedTime" column="last_modified_time" jdbcType="TIMESTAMP"/>
        <result property="expireTime" column="expire_time" jdbcType="TIMESTAMP"/>
        <result property="validTime" column="valid_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Table_Name">
        sys_resource_info
    </sql>

    <sql id="Base_Column_List">
                id,tenant_id,resource_id,resource_type,link_url,status,user_id,remark,create_time,last_modified_time,expire_time,valid_time    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="SysResourceInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="SysResourceInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="resourceId != null and resourceId !=''">and resource_id = #{resourceId}</if>
            <if test="resourceType != null and resourceType !=''">and resource_type = #{resourceType}</if>
            <if test="linkUrl != null and linkUrl !=''">and link_url = #{linkUrl}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="userId != null and userId !=''">and user_id = #{userId}</if>
            <if test="remark != null and remark !=''">and remark = #{remark}</if>
            <if test="expireTime != null and expireTime !=''">and expire_time = #{expireTime}</if>
            <if test="validTime != null and validTime !=''">and valid_time = #{validTime}</if>
        </where>
    </select>

    <!--查询一条-->
    <select id="selectOne" resultMap="SysResourceInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="resourceId != null and resourceId !=''">and resource_id = #{resourceId}</if>
            <if test="resourceType != null and resourceType !=''">and resource_type = #{resourceType}</if>
            <if test="linkUrl != null and linkUrl !=''">and link_url = #{linkUrl}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="userId != null and userId !=''">and user_id = #{userId}</if>
            <if test="remark != null and remark !=''">and remark = #{remark}</if>
            <if test="expireTime != null and expireTime !=''">and expire_time = #{expireTime}</if>
            <if test="validTime != null and validTime !=''">and valid_time = #{validTime}</if>
        </where>
    </select>

    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="SysResourceInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="resourceId != null and resourceId !=''">and resource_id like CONCAT(CONCAT('%', #{resourceId}),
                '%')
            </if>
            <if test="resourceType != null and resourceType !=''">and resource_type like CONCAT(CONCAT('%',
                #{resourceType}), '%')
            </if>
            <if test="linkUrl != null and linkUrl !=''">and link_url like CONCAT(CONCAT('%', #{linkUrl}), '%')</if>
            <if test="status != null and status !=''">and status like CONCAT(CONCAT('%', #{status}), '%')</if>
            <if test="userId != null and userId !=''">and user_id like CONCAT(CONCAT('%', #{userId}), '%')</if>
            <if test="remark != null and remark !=''">and remark like CONCAT(CONCAT('%', #{remark}), '%')</if>
            <if test="expireTime != null and expireTime !=''">and expire_time like CONCAT(CONCAT('%', #{expireTime}),
                '%')
            </if>
            <if test="validTime != null and validTime !=''">and valid_time like CONCAT(CONCAT('%', #{validTime}), '%')
            </if>
        </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="resourceId != null and resourceId !=''">and resource_id = #{resourceId}</if>
            <if test="resourceType != null and resourceType !=''">and resource_type = #{resourceType}</if>
            <if test="linkUrl != null and linkUrl !=''">and link_url = #{linkUrl}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="userId != null and userId !=''">and user_id = #{userId}</if>
            <if test="remark != null and remark !=''">and remark = #{remark}</if>
            <if test="expireTime != null and expireTime !=''">and expire_time = #{expireTime}</if>
            <if test="validTime != null and validTime !=''">and valid_time = #{validTime}</if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (tenant_id ,resource_id ,resource_type ,link_url ,user_id ,remark ,expire_time ,valid_time )
        values
        (#{tenantId},#{resourceId},#{resourceType},#{linkUrl},#{userId},#{remark},#{expireTime},#{validTime})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( tenant_id ,resource_id ,resource_type ,link_url ,user_id ,remark ,expire_time ,valid_time)
        values
        <foreach collection="entities" item="entity" separator=",">
            ( #{entity.tenantId} ,#{entity.resourceId} ,#{entity.resourceType} ,#{entity.linkUrl} ,#{entity.userId}
            ,#{entity.remark} ,#{entity.expireTime} ,#{entity.validTime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( tenant_id ,resource_id ,resource_type ,link_url ,status ,user_id ,remark ,expire_time ,valid_time)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.resourceId},#{entity.resourceType},#{entity.linkUrl},#{entity.status},#{entity.userId},#{entity.remark},#{entity.expireTime},#{entity.validTime})
        </foreach>
        on duplicate key update
        tenant_id=values(tenant_id) ,resource_id=values(resource_id) ,resource_type=values(resource_type)
        ,link_url=values(link_url) ,status=values(status) ,user_id=values(user_id) ,remark=values(remark)
        ,expire_time=values(expire_time) ,valid_time=values(valid_time)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update
        <include refid="Table_Name"/>
        <set>
            <if test="tenantId != null and tenantId != ''">tenant_id = #{tenantId},</if>
            <if test="resourceId != null and resourceId != ''">resource_id = #{resourceId},</if>
            <if test="resourceType != null and resourceType != ''">resource_type = #{resourceType},</if>
            <if test="linkUrl != null and linkUrl != ''">link_url = #{linkUrl},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="userId != null and userId != ''">user_id = #{userId},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
