<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuoding.wechat.common.dao.sys.SysPageFlowInfoDao">

    <resultMap type="com.nuoding.wechat.common.entity.sys.SysPageFlowInfoEntity" id="SysPageFlowInfoMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="pageId" column="page_id" jdbcType="VARCHAR"/>
        <result property="tenantId" column="tenant_id" jdbcType="VARCHAR"/>
        <result property="flowId" column="flow_id" jdbcType="VARCHAR"/>
        <result property="flowSort" column="flow_sort" jdbcType="VARCHAR"/>
        <result property="status" column="status" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="lastModifiedTime" column="last_modified_time" jdbcType="TIMESTAMP"/>
        <result property="channel" column="channel" jdbcType="VARCHAR"/>
        <result property="controlFlag" column="control_flag" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Table_Name">
        sys_page_flow_info
    </sql>

    <sql id="Base_Column_List">
        id,page_id,tenant_id,flow_id,flow_sort,status,create_time,last_modified_time,channel,control_flag    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="SysPageFlowInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="SysPageFlowInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="pageId != null and pageId !=''">and page_id = #{pageId}</if>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="flowId != null and flowId !=''">and flow_id = #{flowId}</if>
            <if test="flowSort != null and flowSort !=''">and flow_sort = #{flowSort}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="channel != null and channel !=''">and channel = #{channel}</if>
            <if test="controlFlag != null and controlFlag !=''">and control_flag = #{controlFlag}</if>
        </where>
    </select>

    <!--查询一条-->
    <select id="selectOne" resultMap="SysPageFlowInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="pageId != null and pageId !=''">and page_id = #{pageId}</if>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="flowId != null and flowId !=''">and flow_id = #{flowId}</if>
            <if test="flowSort != null and flowSort !=''">and flow_sort = #{flowSort}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="channel != null and channel !=''">and channel = #{channel}</if>
            <if test="controlFlag != null and controlFlag !=''">and control_flag = #{controlFlag}</if>
        </where>
    </select>

    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="SysPageFlowInfoMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="pageId != null and pageId !=''">and page_id like CONCAT(CONCAT('%', #{pageId}), '%')</if>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="flowId != null and flowId !=''">and flow_id like CONCAT(CONCAT('%', #{flowId}), '%')</if>
            <if test="flowSort != null and flowSort !=''">and flow_sort like CONCAT(CONCAT('%', #{flowSort}), '%')</if>
            <if test="status != null and status !=''">and status like CONCAT(CONCAT('%', #{status}), '%')</if>
            <if test="channel != null and channel !=''">and channel like CONCAT(CONCAT('%', #{channel}), '%')</if>
            <if test="controlFlag != null and controlFlag !=''">and control_flag like CONCAT(CONCAT('%',
                #{controlFlag}), '%')
            </if>
        </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from
        <include refid="Table_Name"/>
        <where>
            <if test="pageId != null and pageId !=''">and page_id = #{pageId}</if>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="flowId != null and flowId !=''">and flow_id = #{flowId}</if>
            <if test="flowSort != null and flowSort !=''">and flow_sort = #{flowSort}</if>
            <if test="status != null and status !=''">and status = #{status}</if>
            <if test="channel != null and channel !=''">and channel = #{channel}</if>
            <if test="controlFlag != null and controlFlag !=''">and control_flag = #{controlFlag}</if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (page_id,tenant_id,flow_id,flow_sort,channel,control_flag)
        values
        (#{pageId},#{tenantId},#{flowId},#{flowSort},#{channel},#{controlFlag})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( page_id, tenant_id, flow_id, flow_sort, channel, control_flag)
        values
        <foreach collection="entities" item="entity" separator=",">
            ( #{entity.pageId}, #{entity.tenantId}, #{entity.flowId}, #{entity.flowSort}, #{entity.channel},
            #{entity.controlFlag})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        ( page_id, tenant_id, flow_id, flow_sort, status, channel, control_flag)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.pageId},#{entity.tenantId},#{entity.flowId},#{entity.flowSort},#{entity.status},#{entity.channel},#{entity.controlFlag})
        </foreach>
        on duplicate key update
        page_id=values(page_id),
        tenant_id=values(tenant_id),
        flow_id=values(flow_id),
        flow_sort=values(flow_sort),
        status=values(status),
        channel=values(channel),
        control_flag=values(control_flag)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update
        <include refid="Table_Name"/>
        <set>
            <if test="pageId != null and pageId != ''">page_id = #{pageId},</if>
            <if test="tenantId != null and tenantId != ''">tenant_id = #{tenantId},</if>
            <if test="flowId != null and flowId != ''">flow_id = #{flowId},</if>
            <if test="flowSort != null and flowSort != ''">flow_sort = #{flowSort},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="channel != null and channel != ''">channel = #{channel},</if>
            <if test="controlFlag != null and controlFlag != ''">control_flag = #{controlFlag},</if>
        </set>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from
        <include refid="Table_Name"/>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </delete>

</mapper>

