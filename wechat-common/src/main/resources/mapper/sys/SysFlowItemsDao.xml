            <if test="dataType != null and dataType !=''">and data_type like CONCAT(CONCAT('%', #{dataType}), '%')</if>
            <if test="dataValue != null and dataValue !=''">and data_value like CONCAT(CONCAT('%', #{dataValue}), '%')
            </if>
            <if test="dataMatch != null and dataMatch !=''">and data_match like CONCAT(CONCAT('%', #{dataMatch}), '%')
