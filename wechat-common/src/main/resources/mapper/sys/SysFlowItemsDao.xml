    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="SysFlowItemsMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
            <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
            <if test="flowId != null and flowId !=''">and flow_id like CONCAT(CONCAT('%', #{flowId}), '%')</if>
            <if test="itemId != null and itemId !=''">and item_id like CONCAT(CONCAT('%', #{itemId}), '%')</if>
            <if test="itemSort != null and itemSort !=''">and item_sort like CONCAT(CONCAT('%', #{itemSort}), '%')</if>
            <if test="itemType != null and itemType !=''">and item_type like CONCAT(CONCAT('%', #{itemType}), '%')</if>
