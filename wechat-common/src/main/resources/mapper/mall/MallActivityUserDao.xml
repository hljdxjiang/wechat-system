        insert into
        <include refid="Table_Name"/>
        (tenant_id,activity_id,status,userid,create_time,last_modified_time,complate_time)
        values
        (#{tenantId},#{activityId},#{status},#{userid},#{createTime},#{lastModifiedTime},#{complateTime})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (tenant_id,activity_id,status,userid,create_time,last_modified_time,complate_time)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tenantId},#{entity.activityId},#{entity.status},#{entity.userid},#{entity.createTime},#{entity.lastModifiedTime},#{entity.complateTime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
