<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuoding.wechat.common.dao.back.BackSysDictDao">

    <resultMap type="com.nuoding.wechat.common.entity.back.BackSysDictEntity" id="BackSysDictMap">
            <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="dictValue" column="dict_value" jdbcType="VARCHAR"/>
        <result property="dictTitle" column="dict_title" jdbcType="VARCHAR"/>
        <result property="dictType" column="dict_type" jdbcType="VARCHAR"/>
        <result property="dictSort" column="dict_sort" jdbcType="INTEGER"/>
        <result property="status" column="status" jdbcType="VARCHAR"/>
        <result property="remark" column="remark" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="lastModifiedTime" column="last_modified_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Table_Name">
        back_sys_dict
    </sql>

    <sql id="Base_Column_List">
        id,dict_value,dict_title,dict_type,dict_sort,status,remark,create_time,last_modified_time    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="BackSysDictMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BackSysDictMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
                                                        <if test="dictValue != null and dictValue !=''">and dict_value = #{dictValue}</if>
                                                <if test="dictTitle != null and dictTitle !=''">and dict_title = #{dictTitle}</if>
                                                <if test="dictType != null and dictType !=''">and dict_type = #{dictType}</if>
                                                <if test="dictSort != null and dictSort !=''">and dict_sort = #{dictSort}</if>
                                                <if test="status != null and status !=''">and status = #{status}</if>
                                                <if test="remark != null and remark !=''">and remark = #{remark}</if>
                                                            </where>
    </select>

    <!--查询一条-->
    <select id="selectOne" resultMap="BackSysDictMap">
    select
    <include refid="Base_Column_List"/>
    from
    <include refid="Table_Name"/>
    <where>
                                                        <if test="dictValue != null and dictValue !=''">and dict_value = #{dictValue}</if>
                                                <if test="dictTitle != null and dictTitle !=''">and dict_title = #{dictTitle}</if>
                                                <if test="dictType != null and dictType !=''">and dict_type = #{dictType}</if>
                                                <if test="dictSort != null and dictSort !=''">and dict_sort = #{dictSort}</if>
                                                <if test="status != null and status !=''">and status = #{status}</if>
                                                <if test="remark != null and remark !=''">and remark = #{remark}</if>
                                                                </where>
    </select>

    <!--查询指定行数据-->
    <select id="fuzzyQuery" resultMap="BackSysDictMap">
        select
        <include refid="Base_Column_List"/>
        from
        <include refid="Table_Name"/>
        <where>
                                                                                                    <if test="dictValue != null and dictValue !=''">and dict_value like CONCAT(CONCAT('%',  #{dictValue}), '%')</if>
                                                                                                            <if test="dictTitle != null and dictTitle !=''">and dict_title like CONCAT(CONCAT('%',  #{dictTitle}), '%')</if>
                                                                                                            <if test="dictType != null and dictType !=''">and dict_type like CONCAT(CONCAT('%',  #{dictType}), '%')</if>
                                                                                                            <if test="dictSort != null and dictSort !=''">and dict_sort like CONCAT(CONCAT('%',  #{dictSort}), '%')</if>
                                                                                                            <if test="status != null and status !=''">and status like CONCAT(CONCAT('%',  #{status}), '%')</if>
                                                                                                            <if test="remark != null and remark !=''">and remark like CONCAT(CONCAT('%',  #{remark}), '%')</if>
                                                                                                                </where>

    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from
        <include refid="Table_Name"/>
        <where>
                                                <if test="dictValue != null and dictValue !=''">and dict_value = #{dictValue}</if>
                                        <if test="dictTitle != null and dictTitle !=''">and dict_title = #{dictTitle}</if>
                                        <if test="dictType != null and dictType !=''">and dict_type = #{dictType}</if>
                                        <if test="dictSort != null and dictSort !=''">and dict_sort = #{dictSort}</if>
                                        <if test="status != null and status !=''">and status = #{status}</if>
                                        <if test="remark != null and remark !=''">and remark = #{remark}</if>
                                    </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
        (dict_value,dict_title,dict_type,dict_sort,remark,)
        values
        (#{dictValue},#{dictTitle},#{dictType},#{dictSort},#{remark},)
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (        dict_value,        dict_title,        dict_type,        dict_sort,        remark,)
        values
        <foreach collection="entities" item="entity" separator=",">
            (            #{entity.dictValue},            #{entity.dictTitle},            #{entity.dictType},            #{entity.dictSort},            #{entity.remark},)
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into
        <include refid="Table_Name"/>
            (        dict_value,        dict_title,        dict_type,        dict_sort,        status,        remark,)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.dictValue},#{entity.dictTitle},#{entity.dictType},#{entity.dictSort},#{entity.status},#{entity.remark},)
        </foreach>
        on duplicate key update
        dict_value=values(dict_value),
    dict_title=values(dict_title),
    dict_type=values(dict_type),
    dict_sort=values(dict_sort),
    status=values(status),
    remark=values(remark),
        
        </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update
        <include refid="Table_Name"/>
        <set>
    <if test="dictValue != null and dictValue != ''">dict_value = #{dictValue},</if>
    <if test="dictTitle != null and dictTitle != ''">dict_title = #{dictTitle},</if>
    <if test="dictType != null and dictType != ''">dict_type = #{dictType},</if>
    <if test="dictSort != null and dictSort != ''">dict_sort = #{dictSort},</if>
    <if test="status != null and status != ''">status = #{status},</if>
    <if test="remark != null and remark != ''">remark = #{remark},</if>
        </set>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from
        <include refid="Table_Name"/>
        where id = #{id}
        <if test="tenantId != null and tenantId !=''">and tenant_id = #{tenantId}</if>
    </delete>

</mapper>

